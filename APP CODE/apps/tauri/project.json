{
  "name": "tauri",
  "root": "apps/tauri",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo build",
        "cwd": "{projectRoot}"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cargo run",
        "cwd": "{projectRoot}"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run @hackmol/frontend:dev",
          "nx run tauri:serve"
        ],
        "parallel": true
      }
    }
  }
}